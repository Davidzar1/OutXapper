local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/AikaV3rm/UiLib/master/Lib.lua')))() local window = library:CreateWindow("OutXapper v1.00.0") local nickname = window:CreateFolder("Nickname") local fishing = window:CreateFolder("Fishing") local scrolls = window:CreateFolder("Scrolls") _G.SliderColor = Color3.fromRGB(0,136,255) _G.ButtonColor = Color3.fromRGB(0,136,255) _G.ToggleColor = Color3.fromRGB(0,136,255)  nickname:Box("Nickname","string",function(value) game:GetService("ReplicatedStorage").Core.Remote.Remote:FireServer("NameCheck", value) end)  getgenv().nickFont = 1 nickname:Slider("Font",{ min = 1; max = 8; precise = false; },function(value) getgenv().nickFont = value end)  getgenv().nickColor = 1 nickname:Slider("Text Color",{ min = 1; max = 15; precise = false; },function(value) getgenv().nickColor = value end)  nickname:Button("Apply Nickname",function() local args = { [1] = { ["Fonts"] = { [1] = "Fonts", [2] = nickFont, [3] = game:GetService("Players").LocalPlayer.PlayerGui.Naming.Constraint.Base.Frame.Scroll.Frame.Fonts:FindFirstChild(nickFont) }, ["Design"] = { [1] = "Gradients", [2] = nickColor, [3] = game:GetService("Players").LocalPlayer.PlayerGui.Naming.Constraint.Base.Frame.Scroll.Frame.Gradients:FindFirstChild(nickColor) } } } game:GetService("ReplicatedStorage").Core.Remote.Remote:FireServer("NameSet",unpack(args)) end)  nickname:Label(" ",{ TextSize = 25; TextColor = Color3.fromRGB(50,50,50); BgColor = Color3.fromRGB(50,50,50); })  getgenv().nickDelay = 0.1 nickname:Slider("Animation Delay",{ min = 0; max = 1; precise = true; },function(value) getgenv().nickDelay = value end)  nickname:Toggle("Rainbow",function(bool) getgenv().nickRainbow = bool if bool then spawn(function() getgenv().nickColor = 1 while nickRainbow do if nickColor > 14 then getgenv().nickColor = 1 end getgenv().nickColor = 1 + nickColor local args = { [1] = { ["Fonts"] = { [1] = "Fonts", [2] = nickFont, [3] = game:GetService("Players").LocalPlayer.PlayerGui.Naming.Constraint.Base.Frame.Scroll.Frame.Fonts:FindFirstChild(nickFont) }, ["Design"] = { [1] = "Gradients", [2] = nickColor, [3] = game:GetService("Players").LocalPlayer.PlayerGui.Naming.Constraint.Base.Frame.Scroll.Frame.Gradients:FindFirstChild(nickColor) } } } game:GetService("ReplicatedStorage").Core.Remote.Remote:FireServer("NameSet",unpack(args)) wait(nickDelay) end end) end end)  nickname:Toggle("Disco",function(bool) getgenv().nickDisco = bool if bool then spawn(function() while nickDisco do getgenv().nickColor = math.random(2, 15) local args = { [1] = { ["Fonts"] = { [1] = "Fonts", [2] = nickFont, [3] = game:GetService("Players").LocalPlayer.PlayerGui.Naming.Constraint.Base.Frame.Scroll.Frame.Fonts:FindFirstChild(nickFont) }, ["Design"] = { [1] = "Gradients", [2] = nickColor, [3] = game:GetService("Players").LocalPlayer.PlayerGui.Naming.Constraint.Base.Frame.Scroll.Frame.Gradients:FindFirstChild(nickColor) } } } game:GetService("ReplicatedStorage").Core.Remote.Remote:FireServer("NameSet",unpack(args)) wait(nickDelay) end end) end end)  nickname:Toggle("Font Wobble",function(bool) getgenv().nickWobble = bool if bool then spawn(function() while nickWobble do getgenv().nickFont = math.random(1, 8) local args = { [1] = { ["Fonts"] = { [1] = "Fonts", [2] = nickFont, [3] = game:GetService("Players").LocalPlayer.PlayerGui.Naming.Constraint.Base.Frame.Scroll.Frame.Fonts:FindFirstChild(nickFont) }, ["Design"] = { [1] = "Gradients", [2] = nickColor, [3] = game:GetService("Players").LocalPlayer.PlayerGui.Naming.Constraint.Base.Frame.Scroll.Frame.Gradients:FindFirstChild(nickColor) } } } game:GetService("ReplicatedStorage").Core.Remote.Remote:FireServer("NameSet",unpack(args)) wait(nickDelay) end end) end end)  fishing:Toggle("Auto Fish",function(bool) getgenv().autoFish = bool if bool then spawn(function() wait(1) while autoFish do if game.Players.LocalPlayer.Character:FindFirstChild("FishingRod") then local at = game.Players.LocalPlayer.Character.FishingRod.Handle:GetChildren() for i = 1, #at do while at[i]:FindFirstChild("BillboardGui") do if at[i].BillboardGui.Frame["4"].AbsolutePosition.X > 0 then wait(math.random(0.05, 0.2)) mouse1click() print("Reel") end wait(0.1) end end wait(0.3) mouse1click() print("Cast") wait(0.3) end wait(0.1) end end) end end)  scrolls:Slider("Transparency",{ min = 0; max = 100; precise = false; },function(value) getgenv().tp = value / 100 if game.Workspace:FindFirstChild("1") then local loop = game.Workspace["1"]:GetChildren() for i = 1, #loop do if loop[i].Name == "Fern" or loop[i].Name == "Bush" or loop[i].Name == "Rock" or loop[i].Name == "MeshPart" or loop[i].Name == "Plant" then loop[i].Transparency = getgenv().tp end if loop[i].Name == "Tree" or loop[i].Name == "Palm Tree" or loop[i].Name == "Model" or loop[i].Name == "Bushes" then local loop2 = loop[i]:GetChildren() for u = 1, #loop2 do if loop2[u].ClassName == "MeshPart" or loop2[u].ClassName == "Part" or loop2[u].ClassName == "UnionOperation" then loop2[u].Transparency = getgenv().tp end end end end end if game.Workspace:FindFirstChild("2") then local loop = game.Workspace["2"]:GetChildren() for i = 1, #loop do if loop[i].Name == "Fern" or loop[i].Name == "Bush" or loop[i].Name == "Rock" or loop[i].Name == "MeshPart" or loop[i].Name == "Plant" then loop[i].Transparency = getgenv().tp end if loop[i].Name == "Tree" or loop[i].Name == "Palm Tree" or loop[i].Name == "Model" or loop[i].Name == "Bushes" then local loop2 = loop[i]:GetChildren() for u = 1, #loop2 do if loop2[u].ClassName == "MeshPart" or loop2[u].ClassName == "Part" or loop2[u].ClassName == "UnionOperation" then loop2[u].Transparency = getgenv().tp end end end end end if game.Workspace:FindFirstChild("3") then local loop = game.Workspace["3"]:GetChildren() for i = 1, #loop do if loop[i].Name == "Fern" or loop[i].Name == "Bush" or loop[i].Name == "Rock" or loop[i].Name == "MeshPart" or loop[i].Name == "Plant" then loop[i].Transparency = getgenv().tp end if loop[i].Name == "Tree" or loop[i].Name == "Palm Tree" or loop[i].Name == "Model" or loop[i].Name == "Bushes" then local loop2 = loop[i]:GetChildren() for u = 1, #loop2 do if loop2[u].ClassName == "MeshPart" or loop2[u].ClassName == "Part" or loop2[u].ClassName == "UnionOperation" then loop2[u].Transparency = getgenv().tp end end end end end end)  scrolls:Toggle("Beacons",function(bool) getgenv().beaconReload = bool if bool then spawn(function() while beaconReload do if game.Workspace:FindFirstChild("1") then local beacons = game.Workspace["1"]:GetChildren() for i = 1, #beacons do if beacons[i].Name == "BEACON" then beacons[i]:Destroy() end end local loop = game.Workspace["1"]:GetDescendants() for i = 1, #loop do if loop[i].ClassName == "TouchTransmitter" then if loop[i].Parent.Name == "Part" then local beam = Instance.new("Part") beam.Name = "BEACON" beam.Color = Color3.fromRGB(255, 0, 0) beam.Anchored = true beam.CanCollide = false beam.Transparency = 0.5 beam.Size = Vector3.new(1, 1000, 1) beam.Position = loop[i].Parent.Position beam.Parent = game.Workspace["1"] end end end end if game.Workspace:FindFirstChild("2") then local beacons = game.Workspace["2"]:GetChildren() for i = 1, #beacons do if beacons[i].Name == "BEACON" then beacons[i]:Destroy() end end local loop = game.Workspace["2"]:GetDescendants() for i = 1, #loop do if loop[i].ClassName == "TouchTransmitter" then if loop[i].Parent.Name == "Part" then local beam = Instance.new("Part") beam.Name = "BEACON" beam.Color = Color3.fromRGB(255, 0, 0) beam.Anchored = true beam.CanCollide = false beam.Transparency = 0.5 beam.Size = Vector3.new(1, 1000, 1) beam.Position = loop[i].Parent.Position beam.Parent = game.Workspace["2"] end end end end if game.Workspace:FindFirstChild("3") then local beacons = game.Workspace["3"]:GetChildren() for i = 1, #beacons do if beacons[i].Name == "BEACON" then beacons[i]:Destroy() end end local loop = game.Workspace["3"]:GetDescendants() for i = 1, #loop do if loop[i].ClassName == "TouchTransmitter" then if loop[i].Parent.Name == "Part" then local beam = Instance.new("Part") beam.Name = "BEACON" beam.Color = Color3.fromRGB(255, 0, 0) beam.Anchored = true beam.CanCollide = false beam.Transparency = 0.5 beam.Size = Vector3.new(1, 1000, 1) beam.Position = loop[i].Parent.Position beam.Parent = game.Workspace["3"] end end end end wait(0.5) end end) else if game.Workspace:FindFirstChild("1") then local beacons = game.Workspace["1"]:GetChildren() for i = 1, #beacons do if beacons[i].Name == "BEACON" then beacons[i]:Destroy() end end end if game.Workspace:FindFirstChild("2") then local beacons = game.Workspace["2"]:GetChildren() for i = 1, #beacons do if beacons[i].Name == "BEACON" then beacons[i]:Destroy() end end end if game.Workspace:FindFirstChild("3") then local beacons = game.Workspace["3"]:GetChildren() for i = 1, #beacons do if beacons[i].Name == "BEACON" then beacons[i]:Destroy() end end end end end)  
